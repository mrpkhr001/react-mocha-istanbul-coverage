{
  "name": "react-mocha-istanbul-coverage",
  "version": "1.0.0",
  "description": "Simple react test project testing and converage report",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js test/**/*_test.js",
    "test:watch": "npm run test -- --watch",
    "test:karma": "karma start ./karma.config.js",
    "test:run": "karma run"
  },
  "author": "mrpkhr",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.3.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "react-addons-test-utils": "^15.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
